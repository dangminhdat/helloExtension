{
    "manifest_version": 2,

    "name": "Open First Link",
    "description": "This extension will open the first link found in page immediately",
    "version": "1.0",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["lib/jquery-3.2.1.min.js", "content.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}